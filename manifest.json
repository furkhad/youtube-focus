{
  "manifest_version": 3,
  "name": "Ahmed's YouTube Focus",
  "version": "2.0",
  "description": "AI-powered YouTube feed filter using Gemini Nano for intelligent content filtering.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ]
}
